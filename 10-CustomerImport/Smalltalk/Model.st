rImportTest	instanceVariableNames: 'importer system supplierSystem'	classVariableNames: ''	poolDictionaries: ''	category: '10Pines-C17-2-Model'!!SupplierImportTest methodsFor: 'test - system' stamp: 'BrianBokser 11/20/2017 12:09'!newErpSystemWithCustomer	| anERPSystem |	anERPSystem := Environment current newERPSystem.	anERPSystem addCustomer: self existingCustomer.	^ anERPSystem! !!SupplierImportTest methodsFor: 'as yet unclassified ' stamp: 'BrianBokser 11/20/2017 12:38'!pepeSanchezCustomer	| aPepeSanchezCustomer |	aPepeSanchezCustomer := Customer new.	aPepeSanchezCustomer firstName: 'Pepe'.	aPepeSanchezCustomer lastName: 'Sanchez'.	aPepeSanchezCustomer identificationType: 'D'.	aPepeSanchezCustomer identificationNumber: '22333444'.	^ aPepeSanchezCustomer.! !!SupplierImportTest methodsFor: 'tests' stamp: 'BrianBokser 11/18/2017 22:39'!assert: aSupplier hasAddressAt: aStreetName number: aStreetNumber in: aTown zipCode: aZipCode province: aProvince	| aSupplierAddress |		aSupplierAddress := aSupplier addressAt: aStreetName ifNone: [ self fail ].	self assert: aSupplierAddress streetName equals: aStreetName.	self assert: aSupplierAddress streetNumber equals: aStreetNumber.	self assert: aSupplierAddress town equals: aTown.	self assert: aSupplierAddress zipCode equals: aZipCode.	self assert: aSupplierAddress province equals: aProvince.	! !!SupplierImportTest methodsFor: 'tests' stamp: 'BrianBokser 11/18/2017 22:52'!test02	| aSupplierImporter aSupplierInput aSupplier |	aSupplierInput := ReadStream		on:			'S,Supplier1,D,123A,San Martin,3322,Olivos,1636,BsAs'.	aSupplierImporter := SupplierImporter		from: aSupplierInput		on: supplierSystem.	aSupplierImporter import.	aSupplier := supplierSystem		supplierIdentifiedAs: 'D'		numbered: '123'		ifNotOne: [ self fail ].	self		assert: aSupplier		hasAddressAt: 'San Martin'		number: 3322		in: 'Olivos'		zipCode: 1636		province: 'BsAs'! !!SupplierImportTest methodsFor: 'tests' stamp: 'BrianBokser 11/20/2017 12:05'!test04	| aSupplierImporter aSupplierInput anERPSystemWithCustomer |	aSupplierInput := ReadStream		on:			'S,Supplier1,D,123EC,D,200'.	anERPSystemWithCustomer := self newErpSystemWithCustomer.	aSupplierImporter := SupplierImporter		from: aSupplierInput		on: anERPSystemWithCustomer.			self should: [aSupplierImporter import]	raise: Error - MessageNotUnderstood	withExceptionDo: [ :anError | 		self assert: anError messageText equals: SupplierImporter nonExistingCustomerErrorMessage ].	! !!SupplierImportTest methodsFor: 'tests' stamp: 'BrianBokser 11/20/2017 12:14'!test03	| aSupplierImporter aSupplierInput aSupplier anERPSystemWithCustomer |	aSupplierInput := ReadStream		on:			'S,Supplier1,D,123EC,D,5456774'.	anERPSystemWithCustomer := self newErpSystemWithCustomer.	aSupplierImporter := SupplierImporter		from: aSupplierInput		on: anERPSystemWithCustomer.	aSupplierImporter import.	aSupplier := anERPSystemWithCustomer		supplierIdentifiedAs: 'D'		numbered: '123'		ifNotOne: [ self fail ].	self assert: (aSupplier hasCustomer: self existingCustomer).! !!SupplierImportTest methodsFor: 'tests' stamp: 'BrianBokser 11/18/2017 23:20'!test01	| aSupplierImporter aSupplierInput aSupplier |	aSupplierInput := ReadStream on: 'S,Supplier1,D,123'.	aSupplierImporter := SupplierImporter		from: aSupplierInput		on: supplierSystem.	aSupplierImporter import.	self deny: supplierSystem suppliersIsEmpty.	aSupplier := supplierSystem		supplierIdentifiedAs: 'D'		numbered: '123'		ifNotOne: [ self fail ].	self assert: aSupplier name equals: 'Supplier1'.	self assert: aSupplier identificationType equals: 'D'.	self assert: aSupplier identificationNumber equals: '123'! !!SupplierImportTest methodsFor: 'tests' stamp: 'BrianBokser 11/20/2017 11:08'!setUp	supplierSystem := Environment current newSupplierSystem.! !!SupplierImportTest methodsFor: 'tests' stamp: 'BrianBokser 11/20/2017 12:34'!test05	| aSupplierImporter aSupplierInput anERPSystemWithCustomer aSupplier |	aSupplierInput := ReadStream		on:			'S,Supplier1,D,123NC,Pepe,Sanchez,D,22333444'.	anERPSystemWithCustomer := self newErpSystemWithCustomer.	aSupplierImporter := SupplierImporter		from: aSupplierInput		on: anERPSystemWithCustomer.		aSupplierImporter import.		aSupplier := anERPSystemWithCustomer		supplierIdentifiedAs: 'D'		numbered: '123'		ifNotOne: [ self fail ].	self assert: (aSupplier hasCustomer: self pepeSanchezCustomer).	! !!SupplierImportTest methodsFor: 'customers' stamp: 'BrianBokser 11/20/2017 11:34'!existingCustomer	| anExistingCustomer |	anExistingCustomer := Customer new.	anExistingCustomer firstName: 'Pepe'.	anExistingCustomer lastName: 'Sanchez'.	anExistingCustomer identificationType: 'D'.	anExistingCustomer identificationNumber: '5456774'.	^ anExistingCustomer.! !Object subclass: #SupplierImporter	instanceVariableNames: 'line readStream session record newCustomer newSupplier'	classVariableNames: ''	poolDictionaries: ''	category: '10Pines-C17-2-Model'!!SupplierImporter methodsFor: 'initialization' stamp: 'BrianBokser 11/18/2017 20:17'!initializeOn: aReadStream on: aSupplierSystem	readStream := aReadStream.	session := aSupplierSystem.! !!SupplierImporter methodsFor: 'importing - private - address' stamp: 'CosmeFulanito 11/16/2017 20:03'!assertValidAddressRecordSize		record size ~= 6 ifTrue: [ self signalInvalidAddressRecord ].	! !!SupplierImporter methodsFor: 'importing - private - address' stamp: 'BrianBokser 11/18/2017 22:31'!importAddress	| newAddress |	self assertThereIsSupplierForAddress.	self assertValidAddressRecordSize.		newAddress := Address new.	newAddress streetName: record second.	newAddress streetNumber: record third asInteger.	newAddress town: record fourth.	newAddress zipCode: record fifth asInteger.	newAddress province: record sixth.	^ newSupplier addAddress: newAddress! !!SupplierImporter methodsFor: 'importing - private 